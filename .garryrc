# needs docker network ros-net
alias rosmaster="docker run -it --rm --net ros-net --name master ros:kinetic-ros-core roscore"
alias garrylaunch="docker run -it --rm --net ros-net --name garry --env ROS_HOSTNAME=garry --env ROS_MASTER_URI=http://master:11311 ros:garry roslaunch"
alias garrybuild="cd ~/work/garry; docker build --tag ros:garry ."
